<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-default">
      <div class="">
        <div class="modal-header">
          New style for product_set: <%= @product_set.name %>
        </div>
        <div class="panel-body">
          <%= form_for @style, url: product_set_styles_path(@product_set), multipart: true, method: :post, html: { class: 'ui-form' } do |f| %>
            <%= render partial: 'shared/styles/form', locals: {form: f} %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
